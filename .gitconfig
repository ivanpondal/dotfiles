[alias]
    	st = status
    	co = checkout
    	br = branch
    	lg = log -p
    	lol = log --graph --decorate --pretty=oneline --abbrev-commit
    	lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = yellow
	suntracked = cyan
[push]
	default = current
[core]
	editor = vim
